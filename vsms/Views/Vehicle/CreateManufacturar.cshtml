@model VSMS.Data.manufacturar

@{
    ViewBag.Title = "Add Manufacturar";
    Layout = "~/Views/Shared/admin_layout.cshtml";
}

<h2>Add New Manufacturar (Make)</h2>
<br>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
      
        <div class="editor-label">
            @Html.LabelFor(model => model.vehiclemanufacturar)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.vehiclemanufacturar)
            @Html.ValidationMessageFor(model => model.vehiclemanufacturar)
        </div>
        <br>
        <p>
            <input class="btn btn-primary" type="submit" value="Create" />
        </p>
    </fieldset>
}

<br><br>
<div>
    @Html.ActionLink("Back to Manufacturar List", "Manufacturar")
</div>
